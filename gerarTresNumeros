programa {
  inclua biblioteca Util --> u
  funcao inicio() {
    //Gerar senha Ex:65982

    inteiro numero
    cadeia senha 

    para(inteiro y = 1; y <= 5; y++){
      senha = ""
      para(inteiro i = 1; i <= 3; i++){
        numero = u.sorteia(0, 9)
        senha += numero
      
      }

      escreva(senha, "\n")

    }//fim da funÃ§Ã£o inicio
  }
}
